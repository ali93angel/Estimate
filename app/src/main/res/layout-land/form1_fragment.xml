                        android:id="@+id/checkbox3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />
                </LinearLayout>
            </LinearLayout>
