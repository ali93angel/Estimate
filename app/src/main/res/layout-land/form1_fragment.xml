                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="سیفون 125"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_mid_margin"
                        android:gravity="left"
                        android:text="ارزش منطقه"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="اعیان غیرمسکونی"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="تعداد تجاری"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="نوع تخفیف"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="عدم تخفیف فاضلاب"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/editTextTrackNumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <Spinner
                        android:id="@+id/spinner2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />

                    <EditText
                        android:id="@+id/editTextSifoon125"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextArese"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextAianNonMaskooni"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextTedadSaier"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number" />

                    <Spinner
                        android:id="@+id/spinner4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />

                    <CheckBox
                        android:id="@+id/checkbox2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="شناسه قبض"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="قطر انشعاب"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="سیفون 150"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="عرصه کل"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="اعیان کل"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="تعداد سایر"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="تعداد تخفیف"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:gravity="left"
                        android:text="انشعاب غیردائم"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/editTextBillId"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number" />

                    <Spinner
                        android:id="@+id/spinner3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />

                    <EditText
                        android:id="@+id/editTextSifoon150"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextAianKol"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number" />

                    <EditText
                        android:id="@+id/editTextTedadMaskooni"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number" />

                    <EditText
                        android:id="@+id/editTextArzeshMelk"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
