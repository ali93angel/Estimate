<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="rtl"
    tools:context="com.leon.estimate.Fragments.FormFragment">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/activity_large_margin"
            android:layout_marginTop="@dimen/activity_large_margin"
            android:layout_marginRight="@dimen/activity_large_margin"
            android:gravity="center"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/activity_large_margin"
                android:gravity="center"
                android:text="خدمات:"
                android:textColor="@color/blue4"
                android:textSize="@dimen/textSizeLarge"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                tools:ignore="MissingConstraints" />
        </LinearLayout>

        <ScrollView

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/activity_medium_margin"
            android:layout_marginRight="@dimen/activity_medium_margin"
            android:layout_marginBottom="@dimen/activity_mid_margin"
            android:background="@drawable/border_gray_1"
            android:padding="@dimen/activity_mid_padding">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_large_margin"
                android:layout_marginRight="@dimen/activity_large_margin"
                android:orientation="horizontal"
                android:weightSum="3">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/zone"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/user_type"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/sifoon_100"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/sifoon_200"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/ayan_maskooni"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/number_maskooni"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_mid_margin"
                        android:gravity="left"
                        android:text="@string/zarfiat_qaradadi"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/adam_taxfif_ab"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <EditText
                        android:id="@+id/editTextZoneTitle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <Spinner
                        android:id="@+id/spinner1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/editTextSifoon100"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextSifoon200"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextAianMaskooni"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <EditText
                        android:id="@+id/editTextTedadTejari"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />


                    <EditText
                        android:id="@+id/editTextTedadTakhfif"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:background="@drawable/border_gray_1"
                        android:gravity="center"
                        android:inputType="number"
                        android:lines="1"
                        android:maxLines="1"
                        android:singleLine="true" />

                    <CheckBox
                        android:id="@+id/checkbox1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="center" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/shomare_peygiri"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_margin="@dimen/activity_medium_margin"
                        android:gravity="left"
                        android:text="@string/noe_vagozari"
                        android:textColor="@color/blue4"
                        android:textSize="@dimen/textSizeLarge"
                        android:textStyle="bold" />

                    <TextView
